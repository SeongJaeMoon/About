    print('\033[95m' + " ######" + '\033[96m' + "  ########" + '\033[96m' + " ########")
    print('\033[95m' + "##" + '\033[96m' + "       ##" + '\033[96m' + "      ##      ##")
    print('\033[95m' + " ######" + '\033[96m' + "  ######" + '\033[96m' +  " ##      ##")
    print('\033[95m' + "      ##" + '\033[96m' + " ##" + '\033[96m' +      " ##      ##")
    print('\033[95m' + " ######" + '\033[96m' + "  ########" + '\033[96m' + " ########")